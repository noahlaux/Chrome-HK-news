{
  "name": "Chrome HackerNews",
  "version": "1.0.3",
  "manifest_version": 2,
  "description": "Display the most popular Hacker News on a list",
  "browser_action": {
    "default_icon": "icon.gif",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://news.ycombinator.com/",
    "http://www.google-analytics.com/"
  ],
  "update_url": "https://github.com/noahlaux/Chrome-HK-news/blob/master/updateInfo.xml"
}